<div class="bodywrapper">
<h3 id='storyheader'>Listing feeds</h3>



<table>
  <thead>
    <tr class="liner">
      <th>Title</th>
      <th>Site</th>
      <th>Url</th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @feeds.each do |feed| %>
      <tr>
        <td><%= link_to feed.title, feed %></td>
        <td><%= feed.site %></td>
        <td><%= feed.url %></td>
        <td> <%= link_to 'Edit', edit_feed_path(feed) %> |
         <%= link_to 'Destroy', feed, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>


<%= link_to 'New Feed', new_feed_path, class: "button tiny radius" %>
</div>
