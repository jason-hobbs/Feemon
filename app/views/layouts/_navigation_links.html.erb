<!-- <li class="divider"></li> -->
<% if session[:user_id] %>
  <li><%= link_to 'Dashboard', dashboards_path %></li>
  <li><%= link_to profile_image_for(@current_user, size: 35) + ' ' + @current_user.name, @current_user %></li>

<% else %>
  <li><%= link_to 'Sign Up', signup_path %></li>
  <li><%= link_to 'Sign In', new_session_path %></li>
<% end %>
