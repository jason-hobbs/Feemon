<div class="bodywrapper">

  <div class="row left">


  <div class="small-12 columns">
    <h3><%= link_to @entry.title, @entry.link, :target => '_blank' %></h3>
    <p><%= @entry.published.to_time.strftime("%B #{@entry.published.to_time.day.ordinalize}, %Y %I:%M %p") %></p>
    <p><%= raw @entry.description %></p>
  </div>



  </div>
</div>
