<div class="bodywrapper">
<h3>Unread Entries for <%= @feed.title %> for <%= @user.name %> </h3>



<table>
  <thead>
    <tr class="liner">
      <th></th>
      
      
    </tr>
  </thead>

  <tbody>
    <% @unread.each do |entry| %>
      <tr>
        <td><%= link_to entry.entry_title, feed_entry_path(@feed, entry.entry_id) %></td>
        
      </tr>
    <% end %>
  </tbody>
</table>



</div>