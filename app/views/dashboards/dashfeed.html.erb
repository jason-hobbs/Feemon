<div class="bodywrapper">


<div class="row left">

<div class="small-3 columns show-for-medium-up right-collapse">
    <div class="side-nav ">      
      <% @counts.each do |name, amount| %>
        <%= link_to "#{name}: #{amount}", dashfeed_path(@ids[name]) %><br>
      <% end %>
    </div>
  </div>




  <div class="small-12 medium-9 columns">
<h3>Unread Entries for <%= @feed.title %></h3>
<table>
  <thead>
    <tr class="liner">
      <th></th>
      
      
    </tr>
  </thead>

  <tbody>
    <% @unread.each do |entry| %>
      <tr>
        <td><%= link_to entry.entry_title, feed_entry_path(@feed, entry.entry_id) %></td>
        
      </tr>
    <% end %>
  </tbody>
</table>


</div>



</div>
</div>